#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

bool subsets(vector<int> &arr, vector<int> &v, int i, int target, int sum, vector<vector<int>> &ans)
{
    if(i==arr.size())
    {
        if(sum==target)
        {
            ans.push_back(v);
            return true;
        }
        return false;
    }
    v.push_back(arr[i]);
    if(subsets(arr, v, i+1, target, sum+arr[i], ans))
        return true;
    v.pop_back();
    if(subsets(arr, v, i+1, target, sum, ans))
        return true;
    return false;
}

int main()
{
    vector<int> arr = {1,2,3};
    vector<int> v;
    vector<vector<int>> ans;
    subsets(arr, v, 0, 3, 0, ans);
    // sort(ans.begin(), ans.end());
    for(auto arrList : ans)
    {
        for(auto item : arrList)
            cout<<item;
        cout<<endl;
    }
}
